(define-user-function FilterFunc
  (c-name "GdkFilterFunc")
  (return-type "none")
  (parameters
      '("GdkXEvent*" "xevent")
      '("GdkEvent*" "event")
      '("gpointer" "data")
  )
)
(define-user-function EventFunc
  (c-name "GdkEventFunc")
  (return-type "none")
  (parameters
      '("GdkEvent*" "event")
      '("gpointer" "data")
  )
)
(define-user-function InputFunction
  (c-name "GdkInputFunction")
  (return-type "none")
  (parameters
      '("gpointer" "data")
      '("gint" "source")
      '("GdkInputCondition" "condition")
  )
)
(define-user-function PixbufSaveFunc
  (c-name "GdkPixbufSaveFunc")
  (return-type "gboolean")
  (parameters
      '("const-gchar*" "buf")
      '("gsize" "count")
      '("GError**" "error" (out))
      '("gpointer" "data")
  )
)
