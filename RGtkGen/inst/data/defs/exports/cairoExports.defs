;; From ../../../RGtk2/inst/include/RGtk2/cairo.h

(define-function S_cairo_read_func_t
  (in-module "cairo")
  (c-name "S_cairo_read_func_t")
  (return-type "cairo_status_t")
  (parameters
    '("gpointer" "s_closure")
    '("guchar*" "s_data")
    '("guint" "s_length")
  )
)

(define-function asRCairoPath
  (in-module "cairo")
  (c-name "asRCairoPath")
  (return-type "USER_OBJECT_")
  (parameters
    '("cairo_path_t*" "path")
  )
)

(define-function asCCairoPath
  (in-module "cairo")
  (c-name "asCCairoPath")
  (return-type "cairo_path_t*")
  (parameters
    '("USER_OBJECT_" "s_path")
  )
)

(define-function asCCairoGlyph
  (in-module "cairo")
  (c-name "asCCairoGlyph")
  (return-type "cairo_glyph_t*")
  (parameters
    '("USER_OBJECT_" "s_glyph")
  )
)

(define-function asRCairoRectangle
  (in-module "cairo")
  (c-name "asRCairoRectangle")
  (return-type "USER_OBJECT_")
  (parameters
    '("cairo_rectangle_t*" "path")
  )
  (since "1.4.0")
)
(define-function asRCairoRectangleList
  (in-module "cairo")
  (c-name "asRCairoRectangleList")
