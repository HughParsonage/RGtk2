;; -*- scheme -*-


;; notes: 
;;	- cairo_path_data_t is handled specially
;; 	- cairo_content_t needs special values

;; cairo doesn't have objects or boxed types, etc, but I just treat all
;; ref-counted cairo types as object types (but they're NOT GObjects)

;; "object" types

(define-object cairo
	(in-module "cairo")
	(c-name "cairo_t")
)
(define-object font_face
	(in-module "cairo")
	(c-name "cairo_font_face_t")
)
(define-object pattern
	(in-module "cairo")
	(c-name "cairo_pattern_t")
)
(define-object surface
	(in-module "cairo")
	(c-name "cairo_surface_t")
)
(define-object scaled_font
	(in-module "cairo")
	(c-name "cairo_scaled_font_t")
)

(define-object font_options
	(in-module "cairo")
	(c-name "cairo_font_options_t")
)

;; straight structs (pygtk convention = pointers)

(define-pointer matrix
	(in-module "cairo")
	(c-name "cairo_matrix_t")
	(fields
		'("double" "xx")
		'("double" "yx")
		'("double" "xy")
		'("double" "yy")
		'("double" "x0")
		'("double" "y0")
	)
)
