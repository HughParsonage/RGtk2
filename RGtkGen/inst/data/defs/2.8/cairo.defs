)
(define-pointer text_extents
	(in-module "cairo")
	(c-name "cairo_text_extents_t")
	(fields
		'("double" "x_bearing")
		'("double" "y_bearing")
		'("double" "width")
		'("double" "height")
		'("double" "x_advance")
		'("double" "y_advance")
	)
)
(define-pointer font_extents
	(in-module "cairo")
	(c-name "cairo_font_extents_t")
	(fields
		'("double" "ascent")
		'("double" "descent")
		'("double" "height")
		'("double" "max_x_advance")
		'("double" "max_y_advance")
