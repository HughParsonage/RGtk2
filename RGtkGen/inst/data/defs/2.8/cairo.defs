(define-enum status
	(in-module "cairo")
	(c-name "cairo_status_t")
	(gtype-id "CAIRO_TYPE_STATUS")
	(values
		'("success" "CAIRO_STATUS_SUCCESS")
		'("no-memory" "CAIRO_STATUS_NO_MEMORY")
		'("invalid-restore" "CAIRO_STATUS_INVALID_RESTORE")
