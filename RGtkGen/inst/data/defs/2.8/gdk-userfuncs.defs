(define-user-function FilterFunc
  (in-module "Gdk")
  (c-name "GdkFilterFunc")
  (return-type "none")
  (parameters
      '("GdkXEvent*" "xevent")
      '("GdkEvent*" "event")
      '("gpointer" "data")
  )
)
(define-user-function EventFunc
  (in-module "Gdk")
  (c-name "GdkEventFunc")
  (return-type "none")
  (parameters
      '("GdkEvent*" "event")
      '("gpointer" "data")
  )
)
(define-user-function PixbufSaveFunc
  (in-module "Gdk")
  (c-name "GdkPixbufSaveFunc")
  (return-type "gboolean")
  (parameters
      '("const-guchar*" "buf")
      '("gsize" "count")
      '("GError**" "error" (out))
      '("gpointer" "data")
  )
)
(define-user-function SpanFunc
  (in-module "Gdk")
  (c-name "GdkSpanFunc")
  (return-type "none")
  (parameters
      '("GdkSpan*" "span")
      '("gpointer" "data")
  )
)
